PRINT oof "(implicit)" string
WRITE x 0
WHILE <x><10
    SETPX <x> 2 {pusher_down}
    WAIT 500
    WRITE x ~<(<x>+2)
END
PRINT there should be pushers now
IF <x>==10
    PRINT x is 10 and the loop worked
ELIF <x><0
    PRINT AAAAAAAAAAAAAAAAA
ELSE
    PRINT oh no borken
END

